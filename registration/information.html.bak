	function function1(){
		var a=document.getElementById("name").value;
		var e=document.getElementById("email").value;
		var p=document.getElementById("no").value;
		var c=document.getElementById("company_name").value;
		var t1=document.getElementById("design").checked;
	    var t2=document.getElementById("graphics").checked;
	    var t3=document.getElementById("media").checked;
	    var t4=document.getElementById("omg").checked;
		var t5=document.getElementById("mobile").checked;
	    var t6=document.getElementById("iphone").checked;
	    var t7=document.getElementById("deve").checked;
	    var t8=document.getElementById("dsg").checked;
		var t9=document.getElementById("marketing").checked;
		var b=document.getElementById("budget").value;
		var f=document.getElementById("frame").value;
		var d=document.getElementById("description").value;
		var f1=document.getElementById("file").value;
		var nameRegex = /^[a-zA-Z\-]+$/;
		var re=/^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		if(a==""){
			alert("enter your name");
			document.getElementById("name").focus();
			return;
		}
		if (nameRegex.test(a)==false){
			alert("invalid name");
			document.getElementById("name").focus();
			return;
		}
		if(e==""){
			alert("please enter your email");
			document.getElementById("email").focus();
			return;
		}
		if(re.test(e)==false){
			alert("invalid email");
			document.getElementById("email").focus();
			return;
		}
		if(p==""){
			alert("please enter your phone number");
			document.getElementById("no").focus();
			return;
		}
		if(isNaN(p)){
			alert("invalid phone number" );
			document.getElementById("no").focus();
			return;
		}
		if(c==""){
			alert("please enter your Company name");
			document.getElementById("company_name").focus();
			return;
		}
		if (nameRegex.test(c)==false){
			alert("invalid Companyname");
			document.getElementById("company_name").focus();
			return;
		}
		if(t1==false && t2==false && t3==false && t4==false && t5==false && t6==false && t7==false && t8==false && t9==false){
			alert("please select your interested in the  following services");
			return;
		}
		if(b==""){
			alert("please enter your project budget");
			document.getElementById("budget").focus();
			return;
		}
		if(isNaN(b)){
			alert("invalid your project budget" );
			document.getElementById("budget").focus();
			return;
		}
		if(document.getElementById("frame").options[document.getElementById("frame").selectedIndex].value=="select"){
			alert("please select a time frame");
			return;
			
		}
		if(d==""){
			alert("please enter your project description");
			document.getElementById("description").focus();
			return;
		}
		if(f1==""){
			alert("please select the file");
			return;
		}
	}
